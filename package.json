  "scripts": {
    "start": "cross-env craco start",
    "dev:vconsole": "cross-env REACT_APP_API_ENV=vconsole craco start",
    "build": "cross-env CI= GENERATE_SOURCEMAP=false craco build",
    "build:kovan": "cross-env CI= GENERATE_SOURCEMAP=false craco build",
    "pkg:kovan": "rm -rf build* && npm run build:kovan && tar -zcvf awaken-build-kovan.tar.gz build",
    "dev:prod": "cross-env REACT_APP_ENV=prod craco start",
    "build:prod": "cross-env REACT_APP_ENV=prod CI= GENERATE_SOURCEMAP=false craco build",
    "pkg:prod": "rm -rf build* && npm run build:prod && tar -zcvf awaken-build-prod.tar.gz build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "commit": "git-cz",
    "dev:test": "cross-env REACT_APP_API_ENV=test craco start",
    "build:test": "cross-env REACT_APP_API_ENV=test CI= GENERATE_SOURCEMAP=false craco build",
    "dev:local": "cross-env REACT_APP_API_ENV=local craco start",
    "build:local": "cross-env REACT_APP_API_ENV=local CI= GENERATE_SOURCEMAP=false craco build",
    "dev:preview": "cross-env REACT_APP_API_ENV=preview craco start",
    "build:preview": "cross-env REACT_APP_API_ENV=preview CI= GENERATE_SOURCEMAP=false craco build",
    "build:preview:ana": "cross-env REACT_APP_API_ENV=preview CI= WEBPACK_ANA=1 GENERATE_SOURCEMAP=false craco build",
    "pkg:preview": "rm -rf build* && npm run build:preview && tar -zcvf awaken-build-kovan.tar.gz build",
    "prettier:fix": "prettier --config .prettierrc.js --write './**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
